.btnbox{
  display: flex;
  height: 150rpx;
  bottom: 0rpx;
  width: 100%;
  border: none;
}
.btngroup{
  position: fixed;
  bottom: 0;
  width: 100%;
}
button{
  background-color: black;
  color: aliceblue;
  border-radius: 0%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 60rpx;
}
button::after{
  border-radius: 0;
  border: solid;
}
.brown{
  background-color: brown;
}
.c{
  color: brown;
}

.screen{
  word-wrap: break-word;
  width: 100%;
  position: fixed;
  bottom: 900rpx;
  
}
.screen view{
  text-align: right;
  margin: 20rpx;
}


  
.isRuleShow {
  display: block;
}
 
.isRuleHide {
  display: none;
}
 
.float1 {
  height: 100%;
  width: 100%;
  position: fixed;
  background-color: rgb(0, 0, 0,0.5);
  z-index: 2;
  top: 0;
  left: 0;
}

 
.floatContent {
  z-index: 3;
  padding: 20rpx 0;
  width: 80%;
  height: 300rpx;
  background: #fff;
  margin:30% auto;
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  position: relative;
}