<!--miniprogram/pages/myMatrices/myMatrices.wxml-->
<view class="page">

	<view class="body">
		<view class="AB">A</view>
		<view class="picker-section">
			<view class="picker1">
				<picker-view indicator-style="height: 50px;" style="width: 50rpx; height: 180rpx;" value="{{value1}}" bindchange="bindChange1">
					<picker-view-column>
						<view wx:for="{{days}}" wx:key='*this' style="line-height: 50px">{{item}}行</view>
					</picker-view-column>
				</picker-view>
			</view>
			<view class="picker2">
				<picker-view indicator-style="height: 50px;" style="width: 50rpx; height: 180rpx;" value="{{value2}}" bindchange="bindChange2">
