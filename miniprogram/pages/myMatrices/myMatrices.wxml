					</picker-view-column>
				</picker-view>
			</view>
		</view>
		<view class="input-section" wx:if="{{isClear}}">
			<view class="input-box">
				<input class="input" placeholder="0" bindinput="bindinput" id="{{0}}"></input>
				<input class="input" placeholder="0" bindinput="bindinput" id="{{10}}" wx:if="{{row>1}}"></input>
				<input class="input" placeholder="0" bindinput="bindinput" id="{{20}}" wx:if="{{row>2}}"></input>
				<input class="input" placeholder="0" bindinput="bindinput" id="{{30}}" wx:if="{{row>3}}"></input>
