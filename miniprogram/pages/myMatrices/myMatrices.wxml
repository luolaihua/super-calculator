				<input class="input" placeholder="0" bindinput="bindinput" id="{{44}}" wx:if="{{row>4}}"></input>

			</view>

		</view>
	</view>
	<view class="op-section">
		<view class="op-box">
			<view class="opView" bindtap="clickOperator" id="inverse" hover-class="shadow2">+</view>
			<view class="opView" bindtap="clickOperator" id="transpose" hover-class="shadow2">-</view>
			<view class="opView" bindtap="clickOperator" id="det" hover-class="shadow2">det</view>
			<view class="opView" bindtap="clickOperator" id="Q" hover-class="shadow2">QR-Q</view>
		</view>
		<view class="op-box">
			<view class="opView" bindtap="clickOperator" id="det" hover-class="shadow2">*</view>
			<view class="opView" bindtap="clickOperator" id="trace" hover-class="shadow2">./</view>
			<view class="opView" bindtap="clickOperator" id="trace" hover-class="shadow2">trace</view>
			<view class="opView" bindtap="clickOperator" id="R" hover-class="shadow2">QR-R</view>

		</view>
		<view class="op-box">
			<view class="opView" bindtap="clickOperator" id="love" hover-class="shadow2">♡</view>
			<view class="opView" bindtap="clickOperator" id="eigsVector" hover-class="shadow2">.*</view>
			<view class="opView" bindtap="clickOperator" id="transpose" hover-class="shadow2">pow</view>
			<view class="opView" bindtap="clickOperator" id="L" hover-class="shadow2">LU-L</view>

		</view>
		<view class="op-box">
			<view class="opView" bindtap="clickOperator" id="clear" hover-class="shadow2">C</view>
			<view class="opView" bindtap="clickOperator" id="eigsValue" hover-class="shadow2">分式</view>
