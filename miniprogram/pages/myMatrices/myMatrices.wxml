			<view class="opView" bindtap="clickOperator" id="U" hover-class="shadow2">LU-U</view>

		</view>
		<view class="op-box">
			<view class="opView" bindtap="clickOperator" id="clear" hover-class="shadow2">转置</view>
			<view class="opView" bindtap="clickOperator" id="eigsValue" hover-class="shadow2">求逆</view>
			<view class="opView" bindtap="clickOperator" id="transpose" hover-class="shadow2">特征值</view>
			<view class="opView" bindtap="clickOperator" id="transpose" hover-class="shadow2">特征向量</view>
		</view>
	</view>
