				<input class="input" placeholder="0" bindinput="bindinput" id="{{32}}" wx:if="{{row>3}}"></input>
				<input class="input" placeholder="0" bindinput="bindinput" id="{{42}}" wx:if="{{row>4}}"></input>

			</view>
			<view class="input-box" wx:if="{{coloum>3}}">
				<input class="input" placeholder="0" bindinput="bindinput" id="{{03}}"></input>
				<input class="input" placeholder="0" bindinput="bindinput" id="{{13}}" wx:if="{{row>1}}"></input>
				<input class="input" placeholder="0" bindinput="bindinput" id="{{23}}" wx:if="{{row>2}}"></input>
				<input class="input" placeholder="0" bindinput="bindinput" id="{{33}}" wx:if="{{row>3}}"></input>
				<input class="input" placeholder="0" bindinput="bindinput" id="{{43}}" wx:if="{{row>4}}"></input>
			</view>
			<view class="input-box" wx:if="{{coloum>4}}">
